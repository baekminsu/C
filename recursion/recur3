#include<stdio.h>
#include<stdlib.h>
#include<string.h>
#define _CRT_SECURE_NO_WARNINGS

int recur3(int n) {

	if (n <= 10) {
		printf("%d", n);
		return n;
	} //3408  - > 8 0 4 3

	else {
		printf("%d\n",n % 10);
		recur3(n / 10);
	}

}


int main() {
	int n;
	scanf("%d", &n);

	recur3(n); //3408

	return 0;
}
